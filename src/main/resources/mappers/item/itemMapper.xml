<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.study.mappers.itemMapper">
	<!-- 상품리스트 -->
	<select id="mainListItem" resultType="com.study.shopmall.items.vo.ItemVO">
		SELECT 
			item_no, 
			item_image,
			item_name,
			item_price
		FROM
			items
		ORDER BY item_name
	</select>
	
</mapper>
  